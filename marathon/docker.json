{
    "id": "docker",
    "cmd": "python3 -m http.server 8080",
    "instances": 1,
    "cpus": 0.1,
    "mem": 32.0,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "python:3",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 8080,
                "hostPort": 0,
                "servicePort": 0
            }]
        }
    },
    "healthChecks": [{
        "protocol": "HTTP",
        "path": "/"
    }]
}